{
  "name": "tiny-commands",
  "version": "0.1",
  "description": "a chrome commander",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "HH",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "main.css"
      ],
      "js": [        
        "main.js"
      ]
    }
  ],
  "manifest_version": 2
}